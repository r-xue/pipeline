<!-- This is a test procedure for the SRDP selfcalibration pipeline. 

These steps have been skipped to speed up the test:
- hif_restoredata
- hif_mstransform
- hif_findcont
- cube imaging

You are expected to have the _targets.ms and cont.data as input.

-->

<ProcessingProcedure>
	<ProcedureTitle>hifa_cubeimage</ProcedureTitle>

    <!-- import MS: if importonly=True, no calibration shall be performed -->
    <ProcessingCommand>
        <Command xmlns="">hifa_importdata</Command>
        <ParameterSet>
            <Parameter>
              <Keyword xmlns="">datacolumns</Keyword>
              <Value xmlns="">{'data':'regcal_contline_science'}</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>
    <!-- end of import MS-->

    <!-- run selfcal and save selfcal'ed data in 'corrected' if selfcal=true -->
    
    <ProcessingCommand>
        <Command xmlns="">hif_selfcal</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">field</Keyword>
                <Value xmlns=""></Value>
            </Parameter> 
            <Parameter>
                <Keyword xmlns="">spw</Keyword>
                <Value xmlns=""></Value>
            </Parameter>               
        </ParameterSet>
    </ProcessingCommand>

    <!-- end of the selfcal section -->

    <ProcessingCommand>
        <Command xmlns="">hif_makeimlist</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">specmode</Keyword>
                <Value xmlns="">cont</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>    

    <ProcessingCommand>
        <Command xmlns="">hif_makeimages</Command>
        <ParameterSet>
        </ParameterSet>
    </ProcessingCommand>    

</ProcessingProcedure>