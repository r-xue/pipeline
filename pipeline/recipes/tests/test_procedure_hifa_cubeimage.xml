<ProcessingProcedure>

    <ProcedureTitle>hifa_cubeimage</ProcedureTitle>

    <!-- import MS: if importonly=True, no calibration shall be performed -->
    <ProcessingCommand>
        <Command xmlns="">hifa_importdata</Command>
        <ParameterSet>
            <Parameter>
              <Keyword xmlns="">datacolumns</Keyword>
              <Value xmlns="">{'data':'regcal_line_science'}</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>
    <!-- end of import MS-->

    <ProcessingCommand>
        <Command xmlns="">hif_checkproductsize</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">maxcubesize</Keyword>
                <Value xmlns="">40.0</Value>
            </Parameter>
            <Parameter>
                <Keyword xmlns="">maxcubelimit</Keyword>
                <Value xmlns="">60.0</Value>
            </Parameter>
            <Parameter>
                <Keyword xmlns="">maxproductsize</Keyword>
                <Value xmlns="">500.0</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>

    <ProcessingCommand>
        <Command xmlns="">hif_makeimlist</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">specmode</Keyword>
                <Value xmlns="">cube</Value>
            </Parameter>
            <Parameter>
                <Keyword xmlns="">datatype</Keyword>
                <Value xmlns="">best</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>

    <ProcessingCommand>
        <Command xmlns="">hif_makeimages</Command>
        <ParameterSet>
        </ParameterSet>
    </ProcessingCommand>

</ProcessingProcedure>
