<ProcessingProcedure>

    <ProcedureTitle>hifv_contimage</ProcedureTitle>

    <ProcessingCommand>
        <Command xmlns="">hifv_importdata</Command>
        <ParameterSet>
            <Parameter>
              <Keyword xmlns="">datacolumns</Keyword>
              <Value xmlns="">{'data':'regcal_contline_science'}</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>

    <!-- run selfcal and save selfcal'ed data in 'corrected' if selfcal=true -->
    
    <ProcessingCommand>
        <Command xmlns="">hif_selfcal</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">field</Keyword>
                <Value xmlns=""></Value>
            </Parameter>
            <Parameter>
                <Keyword xmlns="">spw</Keyword>
                <Value xmlns=""></Value>
            </Parameter>                     
        </ParameterSet>
    </ProcessingCommand>

    <ProcessingCommand>
        <Command xmlns="">hif_makeimlist</Command>
        <ParameterSet>
            <Parameter>
                <Keyword xmlns="">specmode</Keyword>
                <Value xmlns="">cont</Value>
            </Parameter>
        </ParameterSet>
    </ProcessingCommand>    

    <ProcessingCommand>
        <Command xmlns="">hif_makeimages</Command>
        <ParameterSet>
        </ParameterSet>
    </ProcessingCommand>        

</ProcessingProcedure>